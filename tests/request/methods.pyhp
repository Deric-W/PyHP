#!/usr/bin/pyhp
<html>
    <head>
        <title>Methods</title>
    </head>
    <body>
        <?pyhp
            from collections import OrderedDict as dict
            print("GET, POST, COOKIE and REQUEST are available.")
            print("Their values are:")
            for method in (PyHP.GET, PyHP.POST, PyHP.COOKIE, PyHP.REQUEST):
                print(dict(sorted(method.items())))
        ?>
    </body>
</html>
